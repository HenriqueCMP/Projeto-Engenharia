<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: White;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.projetoengenharia.controller.LoginController">
   <children>
      <ImageView layoutX="105.0" layoutY="-50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../imagens/WhatsApp%20Image%202019-10-30%20at%2021.47.17.jpeg" />
         </image>
      </ImageView>
      <TextField fx:id="txtLogin" layoutX="160.0" layoutY="180.0" prefHeight="40.0" prefWidth="280.0" promptText="Login">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtSenha" layoutX="160.0" layoutY="240.0" prefHeight="40.0" prefWidth="280.0" promptText="Senha">
         <font>
            <Font size="20.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnEntrar" layoutX="320.0" layoutY="297.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Entrar">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnSair" layoutX="160.0" layoutY="297.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Sair">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
